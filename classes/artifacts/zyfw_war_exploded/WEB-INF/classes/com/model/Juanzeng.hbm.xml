<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.model.Juanzeng" table="juanzeng">
        <id name="id" type="integer">
            <column name="id" />
            <generator class="identity" />
        </id>

        <property name="name" type="string">
            <column name="name"  />
        </property>

        <property name="xiangmu" type="string">
            <column name="xiangmu"  />
        </property>

        <property name="je" type="integer">
            <column name="je"  />
        </property>

        <property name="jzsj" type="string">
            <column name="jzsj"  />
        </property>

    </class>
</hibernate-mapping>
